{
    "file": "wire\/modules\/Inputfield\/InputfieldPassword\/InputfieldPassword.module",
    "textdomain": "wire--modules--inputfield--inputfieldpassword--inputfieldpassword-module",
    "translations": {
        "af214972865d03cc4eb63ed9f0b75554": {
            "text": "Postavi zaporku"
        },
        "f5b75010ea8a54b96f8fe7dafac65c18": {
            "text": "slovo"
        },
        "bc2ff9ed57230bca54736f58c0e4f997": {
            "text": "malo slovo"
        },
        "47cbf6a33c1c75ad209ebb39434e8c16": {
            "text": "veliko slovo"
        },
        "29a1470067b045281deaf936a75d8c06": {
            "text": "znamenka"
        },
        "a0daba589a751c3490e8deaff57e90a5": {
            "text": "simbol\/interpunkcija"
        },
        "0c380241031de9b0ff753a076eb3d4da": {
            "text": "ni\u0161ta (onemogu\u0107i sve iznad)"
        },
        "d9c2d86a66aa5a45326c3757f3a272cc": {
            "text": "Trenutna zaporka"
        },
        "3544848f820b9d94a3f3871a382cf138": {
            "text": "Nova zaporka"
        },
        "70d9be9b139893aa6c69b5e77e614311": {
            "text": "Potvrdi"
        },
        "69c6b6ea639e3235de2ac849162c941c": {
            "text": "najmanje %d znakova"
        },
        "4aa7f5956c86c3e23915ec696eb8c6c6": {
            "text": "Minimalni zahtjevi: %s."
        },
        "4bf6db34b23fdef1751e089f21dfb791": {
            "text": "Jo\u0161 ne vrijedi"
        },
        "4bbb8f967da6d1a610596d7257179c2b": {
            "text": "Neispravno"
        },
        "c012e2a8b9e129403f8b93547086500a": {
            "text": "Prekratka"
        },
        "8654375cc9be52c332c401352dbb9ad4": {
            "text": "Previ\u0161e obi\u010dna"
        },
        "e3f1d4a999777988a7c6662b45e550b1": {
            "text": "Ista kao stara"
        },
        "7324e3727807d95037eb19d304fd91ec": {
            "text": "Slaba"
        },
        "a60852f204ed8028c1c58808b746d115": {
            "text": "U redu"
        },
        "0c6ad70beb3a7e76c3fc7adab7c46acc": {
            "text": "Dobro"
        },
        "fcc2d28a33d2df558f18766e067569c6": {
            "text": "Odli\u010dno"
        },
        "a61bbf7ad0ed153cb6c84fe3c8a9f2bb": {
            "text": "Podudaranja"
        },
        "4ff8f01cf1d14a50f9fd06b32fb6b073": {
            "text": "Ne podudaraju se"
        },
        "6dfc77a11e4ea848d807b5365dd43d74": {
            "text": "Prika\u017ei zaporku"
        },
        "941ed970342a3a14cd20351d15f054bd": {
            "text": "Sakrij zaporku"
        },
        "0a836585fecaf2021c1cf7a25eb932c6": {
            "text": "Potrebna je stara zaporka za unos nove."
        },
        "0ce70633a41dc5c5f1143146bfffaa92": {
            "text": "Stara zaporka koju ste unijeli nije ispravna."
        },
        "867cee98fdd724e7d5bc87f61ce657bc": {
            "text": "Zaporke se ne podudaraju"
        },
        "7476d9ca9a945f86c26dfd4d5efa8320": {
            "text": "Potrebna zaporka nije navedena"
        },
        "e00f3b748d9ce505eb2a9c80bedab900": {
            "text": "Zaporka je sadr\u017eavala neva\u017ee\u0107i razmak"
        },
        "5c33e11582c8eb21d8f19cc51084d478": {
            "text": "Zaporka ima manja od potrebnog broja znakova"
        },
        "5cdce68c6dc93aaa78490162327fadd7": {
            "text": "Zaporka ne sadr\u017ei barem jedno slovo (a-z A-Z)"
        },
        "ed3074381758b633a3864645fa28242a": {
            "text": "Zaporka mora sadr\u017eavati barem jedno malo slovo (a-z)"
        },
        "5849909db3843119a232eb7abe586f16": {
            "text": "Zaporka mora imati barem jedno veliko slovo (A-Z)"
        },
        "27d864b5eceddbf8b31385a9c2077caa": {
            "text": "Zaporka ne sadr\u017ei barem jednu znamenku (0-9)"
        },
        "463eaed349f2a4ef4b27982f49ac0393": {
            "text": "Zaporka mora sadr\u017eavati barem jedan znak koji nije slovo, a koji nije znamenka (poput interpunkcijskih znakova)"
        },
        "48f21f57810be80df2c8c3365287e040": {
            "text": "Zahtjevi za zaporku"
        },
        "09978a3eacbaf14a9a3034783eaa625f": {
            "text": "Na\u010din zabrane rije\u010di"
        },
        "bcb96ab1c9d563678a9f5e56be33bb83": {
            "text": "Ako odaberete strogi na\u010din rada, mnoge zaporke koje sadr\u017ee rije\u010di ne\u0107e biti prihva\u0107ene."
        },
        "b0a776318b70ff2500e1a78da0d26446": {
            "text": "Zabrani samo uobi\u010dajene lozinke (preporu\u010deno)"
        },
        "c4ea048bc62d1864591344039210215e": {
            "text": "Zabrani sve lozinke koje sadr\u017ee uobi\u010dajene rije\u010di (strogo)"
        },
        "5b8935e63ff12d77958b22d2348a8402": {
            "text": "Faktor kompleksnosti"
        },
        "de512604c79d42083a531a20da3c4f88": {
            "text": "Ni\u017ei brojevi dopu\u0161taju slabije lozinke, ve\u0107i brojevi zahtijevaju ja\u010de lozinke."
        },
        "684fe1df0fb45c7acb4034164d4c8f9b": {
            "text": "Navedite -1 da biste onemogu\u0107ili ovu zna\u010dajku."
        },
        "bba87093db4f328c614f0586ba426abd": {
            "text": "Preporu\u010dujemo ne\u0161to izme\u0111u 0,5 i 1,0"
        },
        "4c6df844ef940ff03e2c5385cb7628e9": {
            "text": "Minimalna duljina zaporke"
        },
        "72ca0fe877cb728b06ac1a676f706561": {
            "text": "Dopustiti da se postoje\u0107e zaporke prikazuju i\/ili prikazuju u obliku?"
        },
        "a3375ef31a08be286ac6e89e029c82b2": {
            "text": "Tra\u017eiti staru zaporkU prije dopu\u0161tanja promjena?"
        },
        "7edb0ec4f2d7c1fff947ae2f80ba5dfc": {
            "text": "Primjenjuje se na upotrebe u kojima se ovo polje pojavljuje samo ve\u0107 prijavljenim korisnicima."
        },
        "93cba07454f06a4a960172bbd6e2a435": {
            "text": "Da"
        },
        "bafd7322c6e97d25b6299b5d6fe8920b": {
            "text": "Ne"
        },
        "3a46fbb9e3e7d212ab6f9f79415874c2": {
            "text": "Dopustiti korisniku da poka\u017ee\/demaskira zaporku tijekom promjena?"
        },
        "e4d7fc7a63b46e6ad8575eb7242f9d33": {
            "text": "Omogu\u0107uje kontrolu lozinke prika\u017ei\/sakrij tako da korisnici mogu vidjeti \u0161to upisuju kada su u odgovaraju\u0107em okru\u017eenju."
        },
        "dc647eb65e6711e155375218212b3964": {
            "text": "Zaporka"
        },
        "c627446d1f4b534dd3019f3a70be3c59": {
            "text": "Unos zaporke s poljem za potvrdu koje nikad ne ponavlja unos."
        }
    }
}